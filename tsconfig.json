{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/_app.js", "pages/about.js", "pages/index.js", "pages/products/index.js", "pages/products/[product].js", "pages/activities/index.js", "pages/activities/[activity].js", "components/common/PreviewList.js", "components/common/feader.js", "components/common/footer.js"],
  "exclude": ["node_modules"]
}
